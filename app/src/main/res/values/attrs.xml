<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomItemSelectorView">
        <attr name="topBackground" format="reference"></attr>
        <attr name="bottomBackground" format="reference"></attr>
        <attr name="itemHeight" format="dimension"></attr>
        <attr name="itemTextColor" format="color"></attr>
        <attr name="itemTextSize" format="dimension"></attr>
        <attr name="itemTextGravity" format="integer">
            <enum name="leftCenter" value="1" />
            <enum name="centerCenter" value="2" />
            <enum name="rightCenter" value="3" />
        </attr>
        <attr name="itemLineColor" format="color"></attr>
    </declare-styleable>
</resources>
